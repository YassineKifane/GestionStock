<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="418.0" prefWidth="652.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx_project.controllers.EtablissementController">

    <TableView fx:id="EtablissementTableView" prefHeight="418.0" prefWidth="500.0">
        <columns>
            <TableColumn fx:id="idColumn" prefWidth="81" text="ID" />
            <TableColumn fx:id="typeColumn" prefWidth="130" text="Type d'Etablissement" />
            <TableColumn fx:id="nomColumn" prefWidth="130" text="Nom d'Etablissement" />
            <TableColumn fx:id="editColumn" prefWidth="81" text="Edit">
                <cellValueFactory>
                    <PropertyValueFactory property="button" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="deleteColumn" prefWidth="81" text="Delete">
                <cellValueFactory>
                    <PropertyValueFactory property="button" />
                </cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>

    <Button fx:id="addEtablissement" layoutX="508.0" layoutY="44.0" onAction="#openEtablissementForm" styleClass="btn, btn-primary" text="Ajouter Etablissement">
        Add new producer
    </Button>

    <!--   <Button layoutX="508.0" layoutY="353.0" onAction="#handleLogout"  mnemonicParsing="false" prefHeight="25.0" prefWidth="115.0" text="Logout" />-->

</AnchorPane>
