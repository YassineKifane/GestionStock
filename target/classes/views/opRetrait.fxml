<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import javafx.scene.paint.Color?>
<?import javafx.scene.effect.InnerShadow?>
<BorderPane prefHeight="554.0" prefWidth="793.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx_project.controllers.OperationRetraitController">
    <center>
        <AnchorPane prefHeight="42.0" prefWidth="712.0" style="-fx-background-color: #02030A;">
            <children>
            <Label layoutX="14.0" layoutY="24.0" text="Operations de Retrait" textFill="#e7e5e5">
                <font>
                    <Font name="System Bold" size="24.0" />
                </font>
            </Label>
    <TableView fx:id="OperationRetraitTableView" layoutX="1" layoutY="97.0" prefHeight="481.0" prefWidth="795.0">
        <columns>
            <TableColumn fx:id="idColumn" prefWidth="99.0" text="ID" />
            <TableColumn fx:id="categorieColumn" prefWidth="99.0" text="Catégorie" />
            <TableColumn fx:id="designationColumn" prefWidth="99.0" text="Désignation" />
            <TableColumn fx:id="typeColumn" prefWidth="99.0" text="Type d'Etablissement" />
            <TableColumn fx:id="nomColumn" prefWidth="99.0" text="Nom d'Etablissement" />
            <TableColumn fx:id="quantiteColumn" prefWidth="99.0" text="Quantité" />
            <TableColumn fx:id="dateColumn" prefWidth="99.0" text="Date d'Ajout" />
            <TableColumn fx:id="editColumn" prefWidth="99.0" text="Edit">
                <cellValueFactory>
                    <PropertyValueFactory property="button" />
                </cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>


    <Button fx:id="btn" layoutX="508.0" layoutY="353.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="115.0" text="Logout" visible="false" />
                <Button layoutX="570.0" layoutY="19.0" mnemonicParsing="false" onAction="#handleExportButton" prefHeight="53.0" prefWidth="200.0" stylesheets="@style.css" text="Export" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="15.0" />
                    </font>
                    <effect>
                        <InnerShadow>
                            <color>
                                <Color red="0.21052631735801697" green="0.21052631735801697" blue="0.21052631735801697" />
                            </color>
                        </InnerShadow>
                    </effect>
                </Button>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
